    "scripts": {
        "start": "NODE_ENV=production node -r dotenv/config app.js dotenv_config_path=.env.production",
        "dev": "NODE_ENV=development nodemon --exec ts-node -r dotenv/config ./src/app.ts dotenv_config_path=./.env.development",
