        "express": "^4.18.2",
        "handlebars": "^4.7.7",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^6.7.0",
        "multer": "^1.4.5-lts.1",
        "nodemailer": "^6.8.0",
        "nodemailer-smtp-transport": "^2.4.2",
        "sha256": "^0.2.0",
        "sharp": "^0.31.1",
        "uuid": "^9.0.0",
